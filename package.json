{
	"name": "untogethernow",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3005",
		"build": "vite build",
		"preview": "npm run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test:int": "vitest --config vitest.workers.config.ts",
		"test": "npm run test:unit -- --run",
		"deploy": "npm run build && wrangler deploy",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.71",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@inlang/paraglide-js": "^2.2.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-cloudflare": "^7.0.5",
		"@sveltejs/kit": "https://pkg.pr.new/sveltejs/kit/@sveltejs/kit@b921e13",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/browser": "^3.2.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.1.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "https://pkg.pr.new/svelte@15844",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vitest": "~3.2.0",
		"wrangler": "^4.24.3"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.856.0",
		"@aws-sdk/s3-request-presigner": "^3.856.0",
		"bits-ui": "^2.8.13",
		"compressorjs": "^1.2.1",
		"perfect-freehand": "^1.2.2",
		"runed": "^0.31.1",
		"stripe": "^18.3.0",
		"svelte-sonner": "^1.0.5",
		"zod": "^4.0.13"
	}
}
